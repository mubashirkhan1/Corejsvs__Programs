package com.java;

public class Pattern1 extends Thread{
	public run() 
	{
		 for(int i=1; i<=5; i++)
		 {
		System.out.println(Thread.currentThread().getName()+ " "+i);
		 }
	}
		public static void main(String[] args) {
			Pattern1 p=new Pattern1();
			p.start();
			
			 for(int i=1; i<=5; i++)
			 {
					System.out.println(Thread.currentThread().getName()+ " "+i);
				 
			 }
			
		}
		
			
		}
	


