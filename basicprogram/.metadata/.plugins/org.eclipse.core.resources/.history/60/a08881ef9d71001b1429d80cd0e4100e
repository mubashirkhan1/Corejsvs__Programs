package Arrayprograms;

public class SelectionSortingarray {
	public static void main(String[] args) {
		int min;int temp;
		int []a= {6,7,4,9,1,2,4,};
		for(int i=0;i<a.length;i++ ) {
			min=i;
			for(int j=i+1;j<a.length;j++)
			{
				if(a[j]>a[min])
				{
					min=j;
				}
			}
			 temp=a[i];
			 a[i]=a[min];
			 a[min]=a[temp];
			
		}
		for(int i=0;i<a.length-1;i++)
		{
			System.out.println(a[i]+",");
		}
	}

}
