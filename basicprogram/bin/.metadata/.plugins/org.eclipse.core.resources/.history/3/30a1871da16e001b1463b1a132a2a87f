package com.basic;

public class Primenumber {
	public static void main(String[] args) {
		
	
	int no=4,temp=1;
	for(int i=2; i<no-1;i++)
	{
	  if(no%i==0) {
		  temp=temp+1;
		  
	  }
	  
	}
	  if(temp !=1)
	  {
		  System.out.println(no+""+"its not prime");
	  }
	  else {
		  System.out.println("it is a prime no");
		  
	  }
	
	}
}
