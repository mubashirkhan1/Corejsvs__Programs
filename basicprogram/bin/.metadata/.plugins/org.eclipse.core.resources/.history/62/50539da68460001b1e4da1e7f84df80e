package com.java;

public class Tostring1 {
	String name;
	int id;

	Tostring1 (String n,int i){
		id=i;
		name=n;
	Object	
	}

	@Override
	public String toString() {
		return "Tostring1 [name=" + name + ", id=" + id + "]";
	}
	public static void main(String[] args) {
		 Tostring1 t=new  Tostring1 ("ygyu",78);
		 System.out.println(t);
		
	}
}
